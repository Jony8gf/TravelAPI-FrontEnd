    const valorInicial: filtroPeliculasForm = {\n        titulo: '',\n        generoId: 0,\n        proximosEstrenos: false,\n        enCines: false,\n        pagina: 1,\n        recordsPorPagina: 10\n    }\n\n    const [generos, setGeneros] = useState<generoDTO[]>([]);\n    const [peliculas, setPeliculas] = useState<peliculaDTO[]>([]);\n    const history = useHistory();\n    const [errores, setErrores] = useState<string[]>([]);\n    const query = new URLSearchParams(useLocation().search)\n    const [totalDePaginas, setTotalDePaginas] = useState(0);\n\n    useEffect(() => {\n        axios.get(`${urlGeneros}/todos`)\n        .then((respuesta: AxiosResponse<generoDTO[]>) => {\n            setGeneros(respuesta.data);\n        })\n    }, [])\n\n    useEffect(() => {\n\n        if(query.get('titulo')){\n            valorInicial.titulo = query.get('titulo')!;\n        }\n\n        if(query.get('generoId')){\n            valorInicial.generoId = parseInt(query.get('generoId')!, 10);\n        }\n\n        if(query.get('proximosEstrenos')){\n            valorInicial.proximosEstrenos = true;\n        }\n\n        if(query.get('enCines')){\n            valorInicial.enCines = true;\n        }\n\n        if(query.get('pagina')){\n            valorInicial.pagina = parseInt(query.get('pagina')!, 10);\n        }\n\n        buscarPeliculas(valorInicial);\n        //eslint-disable-next-line react-hooks/exhaustive-deps\n    }, [])\n\n    function buscarPeliculas(valores: filtroPeliculasForm){\n        modificarURL(valores);\n        axios.get(`${urlPeliculas}/filtrar`, {params: valores})\n        .then((respuesta: AxiosResponse<peliculaDTO[]>) => {\n            const totalDeRegistros =\n                    parseInt(respuesta.headers['cantidadtotalregistros'], 10);\n            setTotalDePaginas(Math.ceil(totalDeRegistros / valorInicial.recordsPorPagina))\n            setPeliculas(respuesta.data);\n        })\n    }\n\n    function modificarURL(valores: filtroPeliculasForm){\n        const queryStrings: string[] = [];\n        if(valores.titulo){\n            queryStrings.push(`titulo=${valores.titulo}`)\n        }\n\n        if(valores.generoId !==  0){\n            queryStrings.push(`generoId=${valores.generoId}`)\n        }\n\n        if(valores.proximosEstrenos){\n            queryStrings.push(`proximosEstrenos=${valores.proximosEstrenos}`)\n        }\n\n        if(valores.enCines){\n            queryStrings.push(`enCines=${valores.enCines}`)\n        }\n\n        queryStrings.push(`pagina=${valores.pagina}`);\n        history.push(`/peliculas/filtrar?${queryStrings.join('&')}`)\n    }\n\n\n    return (\n        <>\n            <h3>Filtrar Películas</h3>\n\n            <Formik initialValues={valorInicial}\n                onSubmit={valores => {\n                    valores.pagina = 1;\n                    buscarPeliculas(valores)\n                }}\n            >\n                {(formikProps) => (\n                    <>\n                    <Form>\n                        <div className=\"form-inline\">\n                            <div className=\"form-group mb-2\">\n                                <label htmlFor=\"titulo\" className=\"sr-only\">Título</label>\n                                <input type=\"text\"\n                                    className=\"form-control\" id=\"titulo\"\n                                    placeholder=\"Título de la película\"\n                                    {...formikProps.getFieldProps('titulo')}\n                                />\n                            </div>\n                            <div className=\"form-group mx-sm-3 mb-2\">\n                                <select className=\"form-control\"\n                                    {...formikProps.getFieldProps('generoId')}\n                                >\n                                    <option value=\"0\">--Seleccione un género--</option>\n                                    {generos.map(genero => <option key={genero.id}\n                                        value={genero.id}>{genero.nombre}</option>)}\n                                </select>\n                            </div>\n                            <div className=\"form-group mx-sm-3 mb-2\">\n                                <Field className=\"form-check-input\" id=\"proximosEstrenos\"\n                                name=\"proximosEstrenos\" type=\"checkbox\" />\n                                <label className=\"form-check-label\"\n                                htmlFor=\"proximosEstrenos\">Próximos Estrenos</label>\n                            </div>\n                            <div className=\"form-group mx-sm-3 mb-2\">\n                                <Field className=\"form-check-input\" id=\"enCines\"\n                                name=\"enCines\" type=\"checkbox\" />\n                                <label className=\"form-check-label\"\n                                htmlFor=\"enCines\">En Cines</label>\n                            </div>\n                            <Button\n                            className=\"btn btn-primary mb-2 mx-sm-3\"\n                             onClick={() => formikProps.submitForm()}\n                            >Filtrar</Button>\n                            <Button\n                                className=\"btn btn-danger mb-2\"\n                                onClick={() => {\n                                    formikProps.setValues(valorInicial);\n                                    buscarPeliculas(valorInicial)\n                                }}\n                            >Limpiar</Button>\n                        </div>\n                    </Form>\n\n                    <ListadoPeliculas peliculas={peliculas} />\n\n                    <Paginacion\n                        paginaActual={formikProps.values.pagina}\n                        onChange={nuevaPagina => {\n                            formikProps.values.pagina = nuevaPagina;\n                            buscarPeliculas(formikProps.values)\n                        }}\n                        cantidadTotalDePaginas={totalDePaginas}\n                    ></Paginacion>\n                    </>\n                )}\n            </Formik>\n\n            \n\n        </>\n\n    )\n}\n\ninterface filtroPeliculasForm {\n    titulo: string;\n    generoId: number;\n    proximosEstrenos: boolean;\n    enCines: boolean;\n    pagina: number;\n    recordsPorPagina: number;\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\cines\\CrearCines.tsx",["471"],"import axios from \"axios\";\nimport { useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport { urlCines } from \"utils/endpoints\";\nimport { cineCreacionDTO } from \"./cines.model\";\nimport FormularioCines from \"./FormularioCines\";\n\nexport default function CrearCines() {\n\n    const history = useHistory();\n    const [errores, setErrores] = useState<string[]>([]);\n\n    async function crearCine(cine: cineCreacionDTO) {\n        try{\n            await axios.post(urlCines, cine);\n            history.push('/cines');\n        }\n        catch (error){\n            setErrores(error.response.data)\n        }\n    }\n\n    return (\n        <>\n            <h3>Crear cine</h3>\n            <FormularioCines\n                modelo={{nombre: ''}}\n                onSubmit={async valores => {\n                    await crearCine(valores);\n                 }}\n            />\n        </>\n\n    )\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\peliculas\\EditarPeliculas.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\peliculas\\DetallePelicula.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Button.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\auth\\Registro.tsx",["472"],"import axios from \"axios\";\r\nimport { useContext, useState } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { urlCuentas } from \"utils/endpoints\";\r\nimport MostrarErrores from \"utils/MostarErrores\";\r\nimport AutenticationContext from \"./AutenticationConext\";\r\nimport { credencialesUsuario, credencialesUsuarioValidator, respuestaAutenticacion } from \"./auth.model\";\r\nimport FormularioAuth from \"./FormulartioAuth\";\r\nimport { guardarTokenLocalStorage, obtenerClaims } from \"./manejadorJWT\";\r\n\r\nexport default function Registro() {\r\n    \r\n    const history = useHistory();\r\n    const{actualizar} = useContext(AutenticationContext);\r\n    const [errores, setErrores] = useState<string[]>([]);\r\n\r\n    async function Registrar(credenciales:credencialesUsuario) {\r\n        \r\n        try{\r\n            const respuesta = await axios.post<respuestaAutenticacion>(`${urlCuentas}/crear`, credenciales)\r\n            console.log(respuesta);\r\n            guardarTokenLocalStorage(respuesta.data);\r\n            actualizar(obtenerClaims());\r\n            setErrores([]);\r\n            history.push(\"/\");\r\n        }catch(error){\r\n            setErrores(error.response.data);\r\n        }\r\n\r\n        // if(credenciales.password === credenciales.passwordBackup){\r\n        // }else{\r\n        //     errores.push(\"Las contraseñas no coinciden\")\r\n        // } \r\n    }\r\n\r\n    return(\r\n        <>\r\n            <h3>Registro</h3>\r\n            <MostrarErrores errores={errores} />\r\n            <FormularioAuth modelo={{email: '', password: ''}} \r\n                onSubmit={async valores => await Registrar(valores)}\r\n                />\r\n        \r\n        </>\r\n        \r\n    )\r\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\auth\\IndiceUsuarios.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\auth\\Login.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\peliculas\\ListadoPeliculas.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\generos\\FormularioGeneros.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\cines\\FormularioCines.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\actores\\FormularioActores.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\auth\\Autorizado.tsx",["473"],"import { ReactElement, useContext, useEffect, useState } from \"react\";\r\nimport AutenticationContext from \"./AutenticationConext\";\r\n\r\nexport default function Autorizado(props: autorizadoProps){\r\n    \r\n    const[estaAutorizado, setEstaAutorizado] = useState(false);\r\n    const {claims} = useContext(AutenticationContext);\r\n\r\n    useEffect(() => {\r\n        if(props.role){\r\n            const indice = claims.findIndex(claim => claim.nombre === 'role' && claim.valor === props.role)\r\n            setEstaAutorizado(indice > -1);\r\n        }else{\r\n            setEstaAutorizado(claims.length > 0)\r\n        }\r\n    }, [claims])\r\n\r\n    return(\r\n        <>\r\n            {estaAutorizado ? props.autorizado : props.noAutorizado}\r\n        </>\r\n    )\r\n}\r\n\r\ninterface autorizadoProps{\r\n    autorizado: ReactElement;\r\n    noAutorizado?: ReactElement;\r\n    role?: string;\r\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\peliculas\\FormularioPeliculas.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\endpoints.ts",["474"],"const apiURL = process.env.REACT_APP_API_URL;\r\nexport const urlGeneros = `https://localhost:44384/api/Generos`\r\nexport const urlActores = `https://localhost:44384/api/Actores`\r\nexport const urlCines = `https://localhost:44384/api/Cines`\r\nexport const urlPeliculas = `https://localhost:44384/api/Peliculas`\r\nexport const urlCuentas = `https://localhost:44384/api/Cuentas`\r\nexport const urlRatings = `https://localhost:44384/api/Ratings`","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\AlertaConext.ts",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormDataUtils.ts",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\auth\\FormulartioAuth.tsx",["475","476"],"import { Form, Formik, FormikHelpers, yupToFormErrors } from \"formik\";\r\nimport { credencialesUsuario, credencialesUsuarioValidator } from \"./auth.model\";\r\nimport * as Yup from 'yup';\r\nimport FormGroupText from \"utils/FormGroupText\";\r\nimport Button from \"utils/Button\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nexport default function FormularioAuth(props: formularioAuthProps){\r\n\r\n    return(\r\n        <Formik initialValues={props.modelo}\r\n            onSubmit={props.onSubmit}\r\n            validationSchema={Yup.object({\r\n                email: Yup.string().required('Este campo es requerido').email('Debe colocar un email valido'),\r\n                password: Yup.string().required('Este campo es requerido')\r\n            })} \r\n            \r\n            >\r\n            \r\n                {formikProps => (\r\n                    <Form>\r\n                        <FormGroupText label=\"Email\" campo=\"email\" />\r\n                        <FormGroupText label=\"Password\" campo=\"password\" type=\"password\" />\r\n                        {/* <FormGroupText label=\"Repetir Password\" campo=\"rp-password\" type=\"password\" /> */}\r\n\r\n                        <Button disabled={formikProps.isSubmitting} type=\"submit\">Enviar</Button>\r\n                        <Link className=\"btn btn-secondary\" to=\"/\">Cancelar</Link>\r\n                    </Form>\r\n                )}\r\n\r\n\r\n            </Formik>\r\n    )\r\n}\r\n\r\ninterface formularioAuthProps{\r\n    modelo: credencialesUsuario;\r\n    onSubmit(valores: credencialesUsuario, acciones: FormikHelpers<credencialesUsuario>): void\r\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\MostarErrores.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\IndiceEntidad.tsx",["477"],"import axios, { AxiosResponse } from \"axios\";\r\nimport { ReactElement, useEffect, useState } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport Button from \"./Button\";\r\nimport confirmar from \"./Confirmar\";\r\nimport ListadoGenerico from \"./ListadoGenerico\";\r\nimport Paginacion from \"./Paginacion\";\r\n\r\nexport default function IndiceEntidad<T>(props: indiceEntidadProps<T>) {\r\n\r\n    const [entidades, setEntidades] = useState<T[]>();\r\n    const [totalDePaginas, setTotalDePaginas] = useState(0);\r\n    const [recordsPorPagina, setRecordsPorPagina] = useState(10);\r\n    const [pagina, setPagina] = useState(1);\r\n\r\n\r\n    useEffect(() => {\r\n        cargarDatos();\r\n    }, [pagina, recordsPorPagina])\r\n\r\n    function cargarDatos() {\r\n        axios.get(props.url, {\r\n            params: { pagina, recordsPorPagina }\r\n        })\r\n            .then((respuesta: AxiosResponse<T[]>) => {\r\n                const totalDeRegistros =\r\n                    parseInt(respuesta.headers['cantidadtotalregistros'], 10);\r\n                setTotalDePaginas(Math.ceil(totalDeRegistros / recordsPorPagina))\r\n                setEntidades(respuesta.data);\r\n                console.log(respuesta.data)\r\n            })\r\n    }\r\n\r\n    async function borrar(id: number) {\r\n        try {\r\n            await axios.delete(props.url + \"/\" + id);\r\n            cargarDatos();\r\n        } catch (error) {\r\n            console.log(error.response.data)\r\n        }\r\n    }\r\n\r\n    const botones = (urlEditar: string, id: number) => <>\r\n        <Link className=\"btn btn-success mx-2\" to={urlEditar}>Editar</Link>\r\n        <Button\r\n            onClick={() => confirmar(() => borrar(id))}\r\n            className=\"btn btn-danger mx-2\"\r\n        >Borrar</Button>\r\n    </>\r\n\r\n    return (\r\n\r\n        <>\r\n            <h3>{props.titulo}</h3>\r\n            {props.urlCrear ? \r\n                <Link className=\"btn btn-primary\" to={props.urlCrear}>Crear {props.titulo}</Link> \r\n                : null}\r\n\r\n            <select\r\n                className=\"form-control\"\r\n                onChange={e => {\r\n                    setPagina(1);\r\n                    setRecordsPorPagina(Number(e.currentTarget.value))\r\n                }\r\n                }>\r\n                <option value={5}>5</option>\r\n                <option value={10}>10</option>\r\n                <option value={25}>25</option>\r\n                <option value={50}>50</option>\r\n            </select>\r\n\r\n            <Paginacion\r\n                paginaActual={pagina}\r\n                onChange={nuevaPagina => setPagina(nuevaPagina)}\r\n                cantidadTotalDePaginas={totalDePaginas}\r\n            ></Paginacion>\r\n\r\n            <ListadoGenerico listado={entidades}>\r\n                <table className=\"table table-striped\">\r\n                    {props.children(entidades!, botones)}\r\n                </table>\r\n            </ListadoGenerico>\r\n        </>\r\n\r\n\r\n    )\r\n}\r\n\r\ninterface indiceEntidadProps<T> {\r\n    url: string;\r\n    urlCrear?: string;\r\n    children(entidades: T[],\r\n        botones: (urlEditar: string, id: number) => ReactElement): ReactElement;\r\n    titulo: string;\r\n    nombreEntidad?: string;\r\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\EditarEntidad.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Cargando.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Paginacion.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Rating.tsx",["478"],"import { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\r\nimport AutenticationContext from \"auth/AutenticationConext\";\r\nimport { useContext, useEffect, useState } from \"react\"\r\nimport Swal from \"sweetalert2\";\r\nimport './Rating.css'\r\n\r\nexport default function Rating(props: ratingProps) {\r\n\r\n    const {claims} = useContext(AutenticationContext);\r\n    const [maximoValorArr, setMaximoValorArr] = useState<number[]>([]);\r\n    const [valorSeleccionado, setValorSeleccionado] = useState(props.valorSeleccionado);\r\n\r\n    useEffect(() => {\r\n        setMaximoValorArr(Array(props.maximoValor).fill(0));\r\n    }, [props.maximoValor])\r\n\r\n    function manejarMouseOver(voto: number){\r\n        setValorSeleccionado(voto);\r\n    }\r\n\r\n    function manejarClick(voto: number){\r\n        if (claims.length === 0){\r\n            Swal.fire({title: \"Error\", text:'Debes loguearte para votar', icon: 'error'});\r\n            return;\r\n        }\r\n\r\n        setValorSeleccionado(voto);\r\n        props.onChange(voto);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css\" integrity=\"sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==\" crossOrigin=\"anonymous\" referrerPolicy=\"no-referrer\" />\r\n            {maximoValorArr.map((valor, indice) =>\r\n                <i className={`fa-solid fa-star pointer ${valorSeleccionado >= indice + 1 ? 'checked': null}`}\r\n                    key={indice}\r\n                    onClick={() => manejarClick(indice + 1)}\r\n                    onMouseOver={() => manejarMouseOver(indice + 1)}>\r\n                </i>\r\n            )}\r\n        </>\r\n    )\r\n}\r\n\r\ninterface ratingProps {\r\n    maximoValor: number;\r\n    valorSeleccionado: number;\r\n    onChange(voto: number): void;\r\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Mapa.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\ListadoGenerico.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\peliculas\\PeliculaIndividual.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormGroupText.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormGroupMarkdown.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormGroupFecha.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormGroupImagen.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\Confirmar.ts",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\MapaFormulario.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\FormGroupCheckbox.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\SelectorMultiple.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\actores\\TypeAheadActores.tsx",["479"],"import axios, { AxiosResponse } from \"axios\";\nimport { ReactElement, useState } from \"react\";\nimport { AsyncTypeahead, Typeahead } from \"react-bootstrap-typeahead\";\nimport { urlPeliculas } from \"utils/endpoints\";\nimport { actorPeliculaDTO } from \"./actores.model\";\n\nexport default function TypeAheadActores(props: typeAheadActoresProps) {\n\n    const [estaCarganado, setEstaCargando] = useState(false);\n    const [opciones, setOpciones] = useState<actorPeliculaDTO[]>([]);\n    const seleccion: actorPeliculaDTO[] = []\n\n    const [elementoArrastrado, setElementoArrastrado] = \n        useState<actorPeliculaDTO | undefined>(undefined)\n\n    function manejarBusqueda(query: string) \n    {\n        setEstaCargando(true);\n\n        axios.get(urlPeliculas+\"/buscarPorNombre/\"+query)\n            .then((respuesta: AxiosResponse<actorPeliculaDTO[]>) => {\n                setOpciones(respuesta.data);\n                setEstaCargando(false);\n            })\n\n    }\n\n    function manejarDragStart(actor: actorPeliculaDTO) \n    {\n        setElementoArrastrado(actor);\n    }\n\n    function manejarDragOver(actor: actorPeliculaDTO)\n    {\n        if (!elementoArrastrado){\n            return;\n        }\n\n        if (actor.id !== elementoArrastrado.id){\n            const elementoArrastradoIndice = \n                props.actores.findIndex(x => x.id === elementoArrastrado.id);\n            const actorIndice = \n                props.actores.findIndex(x => x.id === actor.id);\n\n            const actores = [...props.actores];\n            actores[actorIndice] = elementoArrastrado;\n            actores[elementoArrastradoIndice] = actor;\n            props.onAdd(actores);\n\n        }\n    }\n\n    return (\n        <>\n            <label>Actores</label>\n            <AsyncTypeahead\n                id=\"typeahead\"\n                onChange={actores => {\n                    if (props.actores.findIndex(x => x.id === actores[0].id) === -1) {\n                        props.onAdd([...props.actores, actores[0]]);\n                    }\n                }}\n                options={opciones}\n                labelKey={actor => actor.nombre}\n                filterBy={() => true}\n                isLoading={estaCarganado}\n                onSearch={manejarBusqueda}\n                placeholder=\"Escriba el nombre del actor...\"\n                minLength={2}\n                flip={true}\n                selected={seleccion}\n                renderMenuItemChildren={actor => (\n                    <>\n                        <img alt=\"imagen actor\" src={actor.foto}\n                            style={{\n                                height: '64px',\n                                marginRight: '10px',\n                                width: '64px'\n                            }}\n                        />\n                        <span>{actor.nombre}</span>\n                    </>\n                )}\n            />\n\n            <ul className=\"list-group\">\n                {props.actores.map(actor => <li\n                    draggable={true}\n                    onDragStart={() => manejarDragStart(actor)}\n                    onDragOver={() => manejarDragOver(actor)}\n                    className=\"list-group-item list-group-item-action\"\n                    key={actor.id}>\n                    {props.listadoUI(actor)}\n                    <span className=\"badge badge-primary badge-pill pointer\"\n                        style={{ marginLeft: '0.5rem' }}\n                        onClick={() => props.onRemove(actor)}\n                    >\n                        X\n                    </span>\n                </li>)}\n            </ul>\n        </>\n    )\n}\n\ninterface typeAheadActoresProps {\n    actores: actorPeliculaDTO[];\n    onAdd(actores: actorPeliculaDTO[]): void;\n    listadoUI(actor: actorPeliculaDTO): ReactElement;\n    onRemove(actor: actorPeliculaDTO): void;\n}","C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-peliculas\\src\\utils\\MostrarErrorCampo.tsx",[],"C:\\Users\\jony8\\OneDrive\\Documentos\\AP.NET-CORE\\react-travel\\src\\index.tsx",["480","481"],"import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport App from './App';\nimport reportWebVitals from './reportWebVitals';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport '../node_modules/bootstrap/dist/css/bootstrap.css'\nimport {library} from '@fortawesome/fontawesome-svg-core'\nimport {faStar} from '@fortawesome/free-solid-svg-icons'\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n\n// If you want to start measuring performance in your app, pass a function\n// t